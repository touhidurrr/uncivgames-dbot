{
  "name": "uncivgames-dbot",
  "version": "2.3.2",
  "description": "A bot owned by touhidurrr made for The Unciv Games Discord Server. Join link https://discord.gg/YYEmec3xr6",
  "module": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "build/esbuild",
    "format": "prettier --write . --config .prettierrc.yaml",
    "wc": "wc $(find src utils -name '*.js' -o -name '*.ts')",
    "deploy": "wrangler deploy",
    "typegen": "bun scripts/typegen.ts",
    "sizecheck": "wrangler deploy --outdir .temp --dry-run && rm -rf .temp"
  },
  "author": "Md. Touhidur Rahman",
  "license": "BSD-3-Clause",
  "engines": {
    "node": "22.x"
  },
  "dependencies": {
    "@libsql/client": "^0.14.0",
    "@prisma/adapter-libsql": "^6.5.0",
    "@prisma/client": "^6.5.0",
    "discord-api-types": "^0.37.119",
    "tweetnacl": "^1.0.3",
    "wrangler": "^4.0.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250313.0",
    "@discordjs/rest": "^2.4.3",
    "@types/bun": "^1.2.5",
    "@types/node": "^22.13.10",
    "prettier": "^3.5.3",
    "prisma": "^6.5.0",
    "typescript": "^5.8.2"
  }
}
