    "tweetnacl": "^1.0.3",
    "wrangler": "^3.106.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250124.3",
    "@discordjs/rest": "^2.4.2",
    "@types/bun": "^1.2.1",
    "@types/node": "^22.12.0",
    "lodash.isequal": "^4.5.0",
