{
  "name": "uncivgames-dbot",
  "version": "3.8.0",
  "license": "BSD-3-Clause",
  "author": "Md. Touhidur Rahman",
  "description": "A bot owned by touhidurrr made for The Unciv Games Discord Server. Join link https://discord.gg/YYEmec3xr6",
  "module": "src/index.js",
  "type": "module",
  "scripts": {
    "format": "prettier --write . --config .prettierrc.yaml",
    "sync": "bun --bun scripts/refreshCommandList.ts",
    "wc": "wc $(find src utils -name '*.js' -o -name '*.ts')",
    "publish": "wrangler deploy",
    "typegen": "bun --bun scripts/typegen.ts",
    "lint": "eslint .",
    "sizecheck": "wrangler deploy --dry-run && rm -rf .wrangler"
  },
  "engines": {
    "node": "22.x"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@prisma/adapter-libsql": "^6.16.3",
    "@prisma/client": "^6.16.3",
    "dayjs": "^1.11.18",
    "discord-api-types": "^0.38.26",
    "wrangler": "^4.41.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251003.0",
    "@discordjs/rest": "^2.6.0",
    "@eslint/js": "^9.36.0",
    "@types/bun": "^1.2.23",
    "@types/node": "^24.7.0",
    "eslint": "^9.36.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "prisma": "^6.16.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0"
  }
}
