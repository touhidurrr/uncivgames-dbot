{
  "name": "uncivgames-dbot",
  "version": "2.3.2",
  "description": "A bot owned by touhidurrr made for The Unciv Games Discord Server. Join link https://discord.gg/YYEmec3xr6",
  "module": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "build/esbuild",
    "format": "prettier --write .",
    "wc": "wc $(find src utils -name '*.js' -o -name '*.ts')",
    "deploy": "wrangler deploy",
    "typegen": "bun scripts/typegen.ts",
    "sizecheck": "wrangler deploy --outdir .temp --dry-run && rm -rf .temp"
