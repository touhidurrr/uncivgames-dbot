  "description": "A bot owned by touhidurrr made for The Unciv Games Discord Server. Join link https://discord.gg/YYEmec3xr6",
  "module": "src/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "publish": "wrangler deploy",
    "typegen": "bun --bun scripts/typegen.ts",
    "sync": "bun --bun scripts/refreshCommandList.ts",
    "format": "prettier --write . --config .prettierrc.yaml",
    "wc": "wc $(find src utils -name '*.js' -o -name '*.ts')",
    "sizecheck": "wrangler deploy --dry-run && rm -rf .wrangler"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@prisma/adapter-libsql": "^6.18.0",
    "@prisma/client": "^6.18.0",
    "dayjs": "^1.11.19",
    "discord-api-types": "^0.38.31",
    "wrangler": "^4.45.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251014.0",
    "@discordjs/rest": "^2.6.0",
    "@eslint/js": "^9.39.0",
    "@types/bun": "^1.3.1",
    "@types/node": "^24.9.2",
    "eslint": "^9.39.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "prisma": "^7.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  }
}
