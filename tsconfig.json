{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "module": "NodeNext",
    "lib": ["ESNext"],
    "types": ["@types/node", "@types/bun", "@cloudflare/workers-types"],
    "paths": {
      "@interactions/*": ["./src/Interactions/*"],
      "@modules/*": ["./src/modules/*"],
      "@src/*": ["./src/*"],

      "@secrets": ["./src/secrets.ts"],
      "@models": ["./src/models.ts"],
      "@lib": ["./src/lib.ts"],
      "@constants": ["./src/constants.ts"]
    }
  }
}
