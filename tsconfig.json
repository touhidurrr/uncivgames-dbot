{
  "compilerOptions": {
    "allowJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "module": "NodeNext",
    "lib": ["ESNext"],
    "types": ["@types/node", "@types/bun", "@cloudflare/workers-types"],
    "paths": {
      "@src/*": ["./src/*"],
      "@modules/*": ["./src/modules/*"],
      "@interactions/*": ["./src/Interactions/*"]
    }
  }
}
